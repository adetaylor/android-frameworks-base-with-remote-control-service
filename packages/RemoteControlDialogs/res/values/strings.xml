<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <string name="prompt"><xliff:g id="app">%s</xliff:g>
        attempts to control this device.
    </string>

    <!-- Dialog message to warn about the risk of using a remote control application. [CHAR LIMIT=NONE] -->
    <string name="warning">This application is attempting to take control of your device.
        This application may access any of your data, impersonate you,
        or do anything else that you can do when using your phone. Are
        you sure that you want to allow remote control?
        <b>Do NOT accept unless you trust the application.</b> Otherwise,
        you run the risk of having your device compromised by malicious
        software.
    </string>

    <!-- Checkbox label to accept the request from a remote control application. [CHAR LIMIT=60] -->
    <string name="accept">I trust this application.</string>

    <!-- Button label to configure the current remote control session. [CHAR LIMIT=20] -->
    <string name="configure">Configure</string>
    <!-- Button label to disconnect the current remote control session. [CHAR LIMIT=20] -->
    <string name="disconnect">Disconnect</string>

    <!-- Label for the name of the current remote control session. [CHAR LIMIT=20] -->
    <string name="session">Session:</string>
    <!-- Label for the duration of the current remote control session. [CHAR LIMIT=20] -->
    <string name="duration">Duration:</string>

    <!-- Dummy string for a blank value. [CHAR LIMIT=40] -->
    <string name="blank_value">--</string>
</resources>
